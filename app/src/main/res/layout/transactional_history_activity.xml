                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.1dp"
                            android:background="@android:color/white"
                            android:gravity="center"
                            android:lines="2"
                            android:text="-"
