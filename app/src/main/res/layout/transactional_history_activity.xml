                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.1dp"
                            android:background="@android:color/white"
                            android:gravity="center"
                            android:lines="2"
                            android:text="2"
                            android:textColor="@color/black"
                            android:textSize="12sp" />

                        <TextView
